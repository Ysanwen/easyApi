{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors":true,
    "sourceMap": false,
    "module": "commonjs",
    "target": "es5",
    "removeComments": true,
    "preserveConstEnums": true,
    "allowJs": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
